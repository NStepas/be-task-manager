{
  "name": "nestjs-starter",
  "version": "0.0.0",
  "description": "my app",
  "author": "Nadiia Stepas",
  "license": "",
  "scripts": {
    "license-check": "license-checker --onlyAllow ';MIT;UNLICENSED;X11;BSD;ISC;Apache-2.0;Apache 2.0;CC-BY-4.0;CC-BY-3.0;CC0-1.0;WTFPL;Unlicense;Apache*' --production --excludePackages ';;'",
    "webpack": "webpack --config webpack.config.js",
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f src/ormconfig.ts",
    "hmr": "node dist/server",
    "inspect:lint": "eslint './src/**/*.ts'",
    "inspect:lint:fix": "eslint './src/**/*.ts' --fix",
    "inspect:test": "jest --passWithNoTests",
    "inspect:test:watch": "jest --watch",
    "inspect:test:cov": "jest --coverage --passWithNoTests",
    "inspect:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "inspect:test:e2e": "jest --config ./test/jest-e2e.json",
    "inspect:license": "license-checker --failOn GPLv2 --summary",
    "inspect:vulnerabilities": "npm audit",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.ts\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.ts\"",
    "inspect:all": "concurrently -c \"bgBlue.bold,bgMagenta.bold,yellow\" \"npm:inspect:test:cov\" \"npm:inspect:lint\" \"npm:inspect:vulnerabilities\" \"npm:inspect:license\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@godaddy/terminus": "4.9.0",
    "@nestjs/common": "7.6.11",
    "@nestjs/config": "0.6.1",
    "@nestjs/core": "7.5.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "7.5.5",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/typeorm": "7.1.5",
    "@types/jsonwebtoken": "8.5.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "crypto-hash": "^2.0.1",
    "csurf": "1.11.0",
    "helmet": "4.2.0",
    "jsonwebtoken": "8.5.1",
    "moment-timezone": "^0.5.34",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "8.6.0",
    "rxjs": "6.6.3",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "0.2.29",
    "typescript": "4.1.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "7.5.5",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.9",
    "@types/helmet": "4.0.0",
    "@types/jest": "26.0.15",
    "@types/joi": "14.3.4",
    "@types/node": "14.14.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "concurrently": "5.3.0",
    "cors": "^2.8.5",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-typescript": "0.14.0",
    "eslint-plugin-unicorn": "23.0.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "joi": "^17.6.0",
    "license-checker": "25.0.1",
    "nodemon": "2.0.6",
    "prettier": "2.2.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
